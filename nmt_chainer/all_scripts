langs=(ar cs de fr)

for x in `seq 1 4`
do
i=$langs[$x]
next=`echo $x+1 | bc`
for y in `seq $next 4`
do
j=$langs[$y]
nextnext=`echo $y+1 | bc`
for z in `seq $nextnext 4`
do
k=$langs[$z]
