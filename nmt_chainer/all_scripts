langs=(ar cs de fr)

for x in `seq 1 3`
do
i=$langs[$x]
next=`echo $x+1 | bc`
for y in `seq $next 3`
do
j=$langs[$y]
if [[ $i != $j ]] && [[ $j == fr ]]
then
echo $i $j
